            <div class="testimonial-container swiper-slide" data-swiper-autoplay="6000">
              <div class="testimonial-content {{ section.settings.content_alignment }}">
                <blockquote>
            {%- if block.settings.title != blank -%}
                    <div class="title-rte">{{ block.settings.title }}</div>
                  {%- endif -%}
                  {%- if block.settings.quote != blank -%}
                    <div class="rte">{{ block.settings.quote }}</div>
                  {%- endif -%}
                  {% render 'dt-sc-rating-widget', block: block %}
                  {%- if block.settings.author != blank -%}
                    <cite>
                      <p class="testimonial-author">{{ block.settings.author }}</p>
                      <span class="designation">{{ block.settings.job_title }}</span>
                    </cite>
